      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <rdf:Description rdf:about="http://www.w3.org/ns/sosa/observes">
            <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/forProperty"/>
            <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
          </rdf:Description>
