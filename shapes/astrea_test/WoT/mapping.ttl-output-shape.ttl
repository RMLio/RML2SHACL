@prefix ns: <http://iot.linkeddata.es/def/wot-mappings#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

<http://mapping.example.com/map_triplesMapDatatypeproperty_000/shape> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://.*" ;
    sh:property [ sh:hasValue "http://www.w3.org/2002/07/owl#DatatypeProperty" ] ;
    sh:targetClass rdfs:Resource,
        "http://www.w3.org/2002/07/owl#DatatypeProperty" .

<http://mapping.example.com/map_triplesMapDatatypepropertymapping_000/shape> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://.*" ;
    sh:property [ sh:hasValue "http://iot.linkeddata.es/def/wot-mappings#DatatypePropertyMapping" ],
        [ sh:node <http://mapping.example.com/map_triplesMapDatatype_000/shape> ;
            sh:path ns:targetDatatype ],
        [ sh:node <http://mapping.example.com/map_triplesMapDatatype_000/shape> ] ;
    sh:targetClass rdfs:Resource,
        "http://iot.linkeddata.es/def/wot-mappings#DatatypePropertyMapping" ;
    sh:targetSubjectsOf ns:targetDatatype .

<http://mapping.example.com/map_triplesMapObjectproperty_000/shape> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://.*" ;
    sh:property [ sh:hasValue "http://www.w3.org/2002/07/owl#ObjectProperty" ] ;
    sh:targetClass rdfs:Resource,
        "http://www.w3.org/2002/07/owl#ObjectProperty" .

<http://mapping.example.com/map_triplesMapObjectpropertymapping_000/shape> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://.*" ;
    sh:property [ sh:hasValue "http://iot.linkeddata.es/def/wot-mappings#ObjectPropertyMapping" ],
        [ sh:node <http://mapping.example.com/map_triplesMapClass_000/shape> ;
            sh:path ns:targetClass ],
        [ sh:node <http://mapping.example.com/map_triplesMapClass_000/shape> ] ;
    sh:targetClass rdfs:Resource,
        "http://iot.linkeddata.es/def/wot-mappings#ObjectPropertyMapping" ;
    sh:targetSubjectsOf ns:targetClass .

<http://mapping.example.com/map_triplesMapProperty_000/shape> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://.*" ;
    sh:property [ sh:hasValue "http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" ] ;
    sh:targetClass rdfs:Resource,
        "http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" .

<http://mapping.example.com/map_triplesMapAccessmapping_000/shape> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://.*" ;
    sh:property [ sh:node <http://mapping.example.com/map_triplesMapEndpoint_000/shape> ;
            sh:path ns:mapsResourcesFrom ],
        [ sh:hasValue "http://iot.linkeddata.es/def/wot-mappings#AccessMapping" ],
        [ sh:node <http://mapping.example.com/map_triplesMapAccessmapping_000/shape> ;
            sh:path ns:isExecutedAfter ],
        [ sh:node <http://mapping.example.com/map_triplesMapMapping_000/shape> ;
            sh:path ns:hasMapping ],
        [ sh:node <http://mapping.example.com/map_triplesMapEndpoint_000/shape> ] ;
    sh:targetClass rdfs:Resource,
        "http://iot.linkeddata.es/def/wot-mappings#AccessMapping" ;
    sh:targetSubjectsOf ns:hasMapping,
        ns:isExecutedAfter,
        ns:mapsResourcesFrom .

<http://mapping.example.com/map_triplesMapMapping_000/shape> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://.*" ;
    sh:property [ sh:nodeKind sh:Literal ;
            sh:path ns:key ],
        [ sh:nodeKind sh:Literal ;
            sh:path ns:jsonPath ],
        [ sh:hasValue "http://iot.linkeddata.es/def/wot-mappings#Mapping" ] ;
    sh:targetClass rdfs:Resource,
        "http://iot.linkeddata.es/def/wot-mappings#Mapping" ;
    sh:targetSubjectsOf ns:jsonPath,
        ns:key .

<http://mapping.example.com/map_triplesMapClass_000/shape> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://.*" ;
    sh:property [ sh:hasValue "http://www.w3.org/2002/07/owl#Class" ] ;
    sh:targetClass rdfs:Resource,
        "http://www.w3.org/2002/07/owl#Class" .

<http://mapping.example.com/map_triplesMapDatatype_000/shape> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://.*" ;
    sh:property [ sh:hasValue "http://www.w3.org/2000/01/rdf-schema#Datatype" ] ;
    sh:targetClass rdfs:Resource,
        "http://www.w3.org/2000/01/rdf-schema#Datatype" .

<http://mapping.example.com/map_triplesMapEndpoint_000/shape> a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://.*" ;
    sh:property [ sh:hasValue "http://iot.linkeddata.es/def/wot#Endpoint" ] ;
    sh:targetClass rdfs:Resource,
        "http://iot.linkeddata.es/def/wot#Endpoint" .

