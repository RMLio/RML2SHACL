  <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Profile">
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://saref.etsi.org/core/Profile">
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:onProperty rdf:resource="https://saref.etsi.org/core/isAbout"/>
