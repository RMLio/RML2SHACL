        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:isDefinedBy>https://saref.etsi.org/core/</rdfs:isDefinedBy>
    <rdfs:label>Measurement</rdfs:label>
  </owl:Class>

