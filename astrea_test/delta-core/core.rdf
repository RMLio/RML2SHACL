<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc11="http://purl.org/dc/elements/1.1/"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <owl:Ontology rdf:about="http://delta.linkeddata.es/def/core">
    <owl:imports rdf:resource="https://w3id.org/def/openadr"/>
    <dc11:title>DELTA ontology</dc11:title>
    <dc:creator rdf:resource="http://www.garcia-castro.com/foaf.rdf#me"/>
    <dc:creator rdf:resource="https://www.linkedin.com/in/albafernandezizquierdo/"/>
    <dc:creator rdf:resource="https://www.linkedin.com/in/andrea-cimmino-arriaga-64b6b9112/"/>
    <dc:license rdf:resource="http://purl.org/NET/rdflicense/cc-by4.0"/>
    <rdfs:comment>This ontology represents the  terms to allow interoperability in DELTA project. It models concepts related to enery market, smart grids and demand-response platforms, and it will reuse standard ontologies whenever possible.</rdfs:comment>
    <owl:versionInfo>0.2.7</owl:versionInfo>
  </owl:Ontology>

  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title">
  </owl:AnnotationProperty>

  <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator">
  </owl:AnnotationProperty>

  <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license">
  </owl:AnnotationProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#agreesOn">
    <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#SmartContract"/>
    <rdfs:range rdf:resource="https://saref.etsi.org/core/Property"/>
    <rdfs:isDefinedBy>Relationship between a smart contract and the property it agrees on.</rdfs:isDefinedBy>
    <rdfs:label>agrees on</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#allocates">
    <owl:inverseOf rdf:resource="http://delta.linkeddata.es/def/core#isAllocatedIn"/>
    <rdfs:isDefinedBy>Relation between a virtual node and its associated aggregator.</rdfs:isDefinedBy>
    <rdfs:label>allocates</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#belongsToCluster">
    <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#VirtualNode"/>
    <rdfs:range>
      <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Cluster">
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasAggregatedProperty">
                <rdfs:comment>Relationship identifying the properties associated to a group of entities, such as clusters.</rdfs:comment>
                <rdfs:label>has aggregated property</rdfs:label>
              </owl:ObjectProperty>
            </owl:onProperty>

            <owl:someValuesFrom rdf:resource="https://saref.etsi.org/core/Property"/>
          </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:onProperty>
              <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#clusterDescription">
                <rdfs:subPropertyOf>
                  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#topDataProperty">
                    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                  </rdf:Description>
                </rdfs:subPropertyOf>

                <rdfs:comment>Description of a cluster</rdfs:comment>
                <rdfs:label>cluster description</rdfs:label>
              </owl:DatatypeProperty>
            </owl:onProperty>

            <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
          </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:onProperty>
              <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#clusterName">
                <rdfs:comment>Name of a cluster</rdfs:comment>
                <rdfs:label>cluster name</rdfs:label>
              </owl:DatatypeProperty>
            </owl:onProperty>

            <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
          </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:comment>Aggrupation of elements, such as FEIDs or Virtual Nodes</rdfs:comment>
        <rdfs:label>Cluster</rdfs:label>
      </owl:Class>
    </rdfs:range>

    <rdfs:comment>Relationship between a virtual node and the cluster it belongs to.</rdfs:comment>
    <rdfs:label>belongs to cluster</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#consumes">
    <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#Aggregator"/>
    <rdfs:range rdf:resource="http://delta.linkeddata.es/def/core#MarketSettlement"/>
    <rdfs:comment>Relation between an aggregator and the market settlement it consumes</rdfs:comment>
    <rdfs:label>consumes</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#detects">
    <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#FEID"/>
    <rdfs:range rdf:resource="http://delta.linkeddata.es/def/core#Violation"/>
    <rdfs:comment>Relationship between the FEID and the violation it detects.</rdfs:comment>
    <rdfs:label>detects</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasBoundary">
    <rdfs:range rdf:resource="http://delta.linkeddata.es/def/core#Boundary"/>
    <rdfs:comment>A relationship defining the boundary values (min and max values) of an entity</rdfs:comment>
    <rdfs:label>has boundary</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasComfortSetting">
    <rdfs:range rdf:resource="http://delta.linkeddata.es/def/core#ComfortSetting"/>
    <rdfs:comment>Relation to define the comfort settings of a system.</rdfs:comment>
    <rdfs:label>has comfort settings</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasCurrency">
    <rdfs:comment>Link between a price and its currency</rdfs:comment>
    <rdfs:label>has currency</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasDeliveryTime">
    <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
    <rdfs:comment>Link to the delivery time.</rdfs:comment>
    <rdfs:label>has delivery time</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasDurationDescription">
    <rdfs:range rdf:resource="http://www.w3.org/2006/time#Duration"/>
    <rdfs:comment>Duration description of a violation, indicanting value and unit.</rdfs:comment>
    <rdfs:label>has duration description</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasEstimatedMeasurement">
    <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#ViolationCondition"/>
    <rdfs:range rdf:resource="http://delta.linkeddata.es/def/core#Measurement"/>
    <rdfs:comment>Relationship between a violation condition and the current associated measurement that is estimated.</rdfs:comment>
    <rdfs:label>has estimated measurement</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasIncentive">
    <rdfs:comment>A relation between a property and its incentives associated.</rdfs:comment>
    <rdfs:label>has incentive</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasIndex">
    <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#VirtualNode"/>
    <rdfs:range rdf:resource="http://delta.linkeddata.es/def/core#Index"/>
    <rdfs:comment>Link between a Virtual Node and its index.</rdfs:comment>
    <rdfs:label>has index</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasLowerBoundary">
    <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#Boundary"/>
    <rdfs:range rdf:resource="http://delta.linkeddata.es/def/core#Measurement"/>
    <rdfs:comment>A relationship defining the lower boundary of an entity (e.g. a comfort setting).</rdfs:comment>
    <rdfs:label>has lower boundary</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasPayment">
    <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#SmartContract"/>
    <rdfs:range rdf:resource="http://delta.linkeddata.es/def/core#Payment"/>
    <rdfs:comment>Relation between a smart contract and the payment associated</rdfs:comment>
    <rdfs:label>has payment</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasPhase">
    <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#Measurement"/>
    <rdfs:range rdf:resource="http://delta.linkeddata.es/def/core#Phase"/>
    <rdfs:comment>Link between a phase and a measurement</rdfs:comment>
    <rdfs:label>has phase</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasProfile">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isProfileOf">
        <rdfs:comment>Relationship between a profile and a device.</rdfs:comment>
        <rdfs:label>is profile of</rdfs:label>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:comment>Link between an entity and its profile</rdfs:comment>
    <rdfs:label>has profile</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasRedundancy">
    <rdfs:comment>Redundancy of  a FEID</rdfs:comment>
    <rdfs:label>has redundancy</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasRequiredMeasurement">
    <rdfs:comment>Relationship between a violation condition and the associated measurement that is required.</rdfs:comment>
    <rdfs:label>has required measurement</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasReward">
    <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#Customer"/>
    <rdfs:comment>Link between a customer and its associated reward</rdfs:comment>
    <rdfs:label>has reward</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasSmartContract">
    <rdfs:range rdf:resource="http://delta.linkeddata.es/def/core#SmartContract"/>
    <rdfs:comment>link between the Virtual node and its smart contracts</rdfs:comment>
    <rdfs:label>has smart contract</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasSystemConstraint">
    <rdfs:comment>Link to the grid constraints of a system</rdfs:comment>
    <rdfs:label>has system constraint</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasTransaction">
    <rdfs:comment>Link between a smart contracts and the transactions it stores</rdfs:comment>
    <rdfs:label>has transaction</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasUpperBoundary">
    <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#Boundary"/>
    <rdfs:range rdf:resource="http://delta.linkeddata.es/def/core#Measurement"/>
    <rdfs:comment>A relationship defining the upper boundary of an entity (e.g. a comfort setting).</rdfs:comment>
    <rdfs:label>has upper boundary</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasViolationCondition">
    <rdfs:comment>Relationship between a violation and all its conditions</rdfs:comment>
    <rdfs:label>has violation condition</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#includesProfile">
    <rdfs:comment>Relationship between a violation and  a profile that describes such violation</rdfs:comment>
    <rdfs:label>includes profile</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isAllocatedIn">
    <rdfs:comment>Link from a virtual node to its aggregator</rdfs:comment>
    <rdfs:label>is allocated in</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isDefinedIn">
    <rdfs:comment>A relationship defining the currency associated to a value</rdfs:comment>
    <rdfs:label>is defined in</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isLocatedAt">
    <rdfs:comment>Relation between an object and the geographical feature in which it is located at</rdfs:comment>
    <rdfs:label>is located at</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isOwnedBy">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#owns">
        <rdfs:comment>Link between the customers and the devices</rdfs:comment>
        <rdfs:label>owns</rdfs:label>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:comment>Link between a device and its customer</rdfs:comment>
    <rdfs:label>is owned by</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isRelatedTo">
    <rdfs:comment>Relationship  between a violation and the related DVNs, FEIDs or Feeders.</rdfs:comment>
    <rdfs:label>isRelatedTo</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isRequestedTo">
    <rdfs:comment>Relationship between  an action and the virtual node in charge of perform it</rdfs:comment>
    <rdfs:label>is requested to</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isValidDuringPeriod">
    <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
    <rdfs:comment>Period where an entity, such a contract, is valid</rdfs:comment>
    <rdfs:label>is valid during period</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#manages">
    <rdfs:comment>Relation between a Virutal Node and the  devices it manages</rdfs:comment>
    <rdfs:label>manages</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#requests">
    <rdfs:comment>Relationship between a grid violation and the requested property to solve it.</rdfs:comment>
    <rdfs:label>requests</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#requiresMeasurement">
    <rdfs:comment>Relationship between a DR request and the needed measurement in order to solve a violation</rdfs:comment>
    <rdfs:label>requires measurement</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.opengis.net/ont/geosparql#hasGeometry">
    <rdfs:domain rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
    <rdfs:range>
      <owl:Class rdf:about="http://www.opengis.net/ont/geosparql#Geometry">
        <rdfs:comment rdf:resource="http://www.opengis.net/ont/geosparql"/>
        <rdfs:comment>The class represents the top-level geometry type. This class is equivalent to the UML class GM_Object defined in ISO 19107, and it is superclass of all geometry types.</rdfs:comment>
        <rdfs:label>Geometry</rdfs:label>
      </owl:Class>
    </rdfs:range>

    <rdfs:comment>A spatial representation for a given feature.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.opengis.net/ont/geosparql"/>
    <rdfs:label>hasGeometry</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/consistsOf">
    <rdfs:comment>A relationship indicating a composite entity that consists of other entities (e.g., a temperature/humidity sensor that consists of a temperature sensor and a humidity sensor)</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/core"/>
    <rdfs:label>consists of</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isAbout">
    <rdfs:comment>A relationship identifying what an entity, such as a Profile, is about</rdfs:comment>
    <rdfs:isDefinedBy>https://saref.etsi.org/core</rdfs:isDefinedBy>
    <rdfs:label>isAbout</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isMeasuredIn">
    <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
    <rdfs:label>is measured in</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isMeasurementOf">
    <rdfs:comment>A relationship between a measurement and the feature of interest whose quality was measured</rdfs:comment>
    <rdfs:isDefinedBy>https://saref.etsi.org/core/</rdfs:isDefinedBy>
    <rdfs:label>isMeasurementOf</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/makesMeasurement">
    <rdfs:comment>A relationship specifying the property that can be measured by a certain device</rdfs:comment>
    <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
    <rdfs:label>makes measurement</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/measuresProperty">
    <rdfs:comment>A relationship specifying the property that can be measured by a certain device</rdfs:comment>
    <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
    <rdfs:label>measures property</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/relatesToMeasurement">
    <rdfs:comment>A relationship between a property and the measurements it relates to</rdfs:comment>
    <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
    <rdfs:label>relates to measurement</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/relatesToProperty">
    <rdfs:comment>A relationship between a measurement and the property it relates to</rdfs:comment>
    <rdfs:isDefinedBy>https://saref.etsi.org/core</rdfs:isDefinedBy>
    <rdfs:label>relates to property</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4city/isDerivedFrom">
    <rdfs:comment>is derived from</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/saref4city"/>
    <rdfs:label>is derived from</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4city/quantifiesKPI">
    <rdfs:comment>quantifies KPI</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/saref4city/v1.1.2/"/>
    <rdfs:label>quantifies KPI</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://w3id.org/def/openadr#belongsTo">
    <rdfs:comment>Relationship between a VE, Resource or Asset with the Group of targets associated</rdfs:comment>
    <rdfs:label>belongs to</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://w3id.org/def/openadr#hasDuration">
    <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
    <rdfs:comment>Relationship between an event and its duration</rdfs:comment>
    <rdfs:label>has duration</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://w3id.org/def/openadr#sendsEvent">
    <rdfs:comment>A link between a node and the event it sends</rdfs:comment>
    <rdfs:label>sends event</rdfs:label>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasCreationDateTime">
    <rdfs:comment>Creation date of a profile</rdfs:comment>
    <rdfs:label>has creation date time</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasDescription">
    <rdfs:label>description</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasGranularity">
    <rdfs:comment>Granularity of the profile (e.g., weekly, daily)</rdfs:comment>
    <rdfs:label>has granularity</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasHref">
    <rdfs:comment>URI of the endpoint</rdfs:comment>
    <rdfs:label>has href</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasIp">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>IP address of a thing</rdfs:comment>
    <rdfs:label>has ip</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasPort">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:comment>Address of the service within the system.</rdfs:comment>
    <rdfs:label>has port</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasReliability">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:comment>Reliability associated to a virtual node</rdfs:comment>
    <rdfs:label>has reliability</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasRewardValue">
    <rdfs:comment>A relationship defining the value of a certain reward, e.g., points or money</rdfs:comment>
    <rdfs:label>has reward value</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasViolationType">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf>
          <rdf:List>
            <rdf:first>overloading</rdf:first>
            <rdf:rest>
              <rdf:List>
                <rdf:first>overvoltage</rdf:first>
                <rdf:rest>
                  <rdf:List>
                    <rdf:first>undervoltage</rdf:first>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </rdf:List>
                </rdf:rest>

              </rdf:List>
            </rdf:rest>

          </rdf:List>
        </owl:oneOf>

      </rdfs:Datatype>
    </rdfs:range>

    <rdfs:comment>Relationship between a violation and its type, which can be overvoltage, undervoltage or overloading.</rdfs:comment>
    <rdfs:label>has violation type</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasViolationValue">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:label>Value of the violation</rdfs:label>
    <rdfs:label>has violation value</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#implementsDRType">
    <rdfs:range>
