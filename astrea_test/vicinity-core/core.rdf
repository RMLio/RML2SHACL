  <owl:Class rdf:about="http://www.opengis.net/ont/geosparql#SpatialObject">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.opengis.net/ont/geosparql#sfContains"/>
