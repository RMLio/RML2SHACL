<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc11="http://purl.org/dc/elements/1.1/"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:ns0="http://purl.org/vocab/vann/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:prov="http://www.w3.org/ns/prov#"
         xmlns:ns1="http://www.w3.org/2003/06/sw-vocab-status/ns#">

  <owl:Ontology rdf:about="http://iot.linkeddata.es/def/core#">
    <owl:imports rdf:resource="http://iot.linkeddata.es/def/wot#"/>
    <owl:imports rdf:resource="http://www.w3.org/ns/org#"/>
    <owl:imports rdf:resource="http://www.w3.org/ns/sosa/"/>
    <dc11:publisher rdf:resource="http://www.oeg-upm.net/"/>
    <dc11:title xml:lang="en">Vicinity core model</dc11:title>
    <dc:contributor rdf:resource="http://www.oeg-upm.net/index.php/en/universitystaff/402-fserena/"/>
    <dc:creator rdf:resource="http://purl.org/net/mpoveda"/>
    <dc:creator rdf:resource="http://www.garcia-castro.com/foaf.rdf#me"/>
    <dc:license rdf:resource="http://purl.org/NET/rdflicense/cc-by4.0"/>
    <ns0:preferredNamespacePrefix>vcnt-core</ns0:preferredNamespacePrefix>
    <ns0:preferredNamespaceUri>http://iot.linkeddata.es/def/core</ns0:preferredNamespaceUri>
    <rdfs:comment xml:lang="en">This ontology represents the core terms to allow interoperability in an IoT context based on  VICINITY technological solutions.</rdfs:comment>
    <owl:versionInfo>0.2.4</owl:versionInfo>
  </owl:Ontology>

  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator">
  </owl:AnnotationProperty>

  <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/source">
  </owl:AnnotationProperty>

  <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/contributor">
  </owl:AnnotationProperty>

  <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#cardinality">
  </owl:AnnotationProperty>

  <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/06/sw-vocab-status/ns#term_status">
  </owl:AnnotationProperty>

  <owl:AnnotationProperty rdf:about="http://www.w3.org/ns/prov#hadPrimarySource">
  </owl:AnnotationProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#aboutFeatureOfInterest">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">Auxiliary property created for the n-ary class core:FeatureProperty to link instances of such class to the feature of interest it is about.</rdfs:comment>
    <rdfs:label xml:lang="en">about feature of interest</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#aboutProperty">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">Auxiliary property created for the n-ary class core:FeatureProperty to link instances of such class to the (ssn) proeprty it is about.</rdfs:comment>
    <rdfs:label xml:lang="en">about property</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#actsOnFeatureProperty">
    <rdfs:range rdf:resource="http://iot.linkeddata.es/def/core#FeatureProperty"/>
    <rdfs:comment xml:lang="en">Auxiliary property created for the n-ary class core:FeatureProperty. This property links devices (sensor or actuators) or interaction patterns to instances of the n-ary class which will be the ones linking to the properties and features of interest which the device or interaction pattern acts on.</rdfs:comment>
    <rdfs:label xml:lang="en">acts on feature property</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#avatar">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/depiction">
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>

    <rdfs:comment xml:lang="en">A link from an entity to the image identifying it in a social network or system.</rdfs:comment>
    <rdfs:label xml:lang="en">avatar</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#describes">
    <owl:inverseOf rdf:resource="http://iot.linkeddata.es/def/core#isDescribedBy"/>
    <rdfs:comment xml:lang="en">Link from a thing description to the thing being described</rdfs:comment>
    <rdfs:label xml:lang="en">describes thing</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#describesEcosystem">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#isEcosystemDescribedBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">Link from an ecosystem to the thing ecosystem descriptions that provide information about such ecosystem.</rdfs:comment>
        <rdfs:label xml:lang="en">is ecosystem described by</rdfs:label>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:comment xml:lang="en">Link between a thing ecosystem description and the ecosystem being described.</rdfs:comment>
    <rdfs:label xml:lang="en">describes ecosystem</rdfs:label>
    <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#exposes">
    <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#VirtualThing"/>
    <rdfs:range rdf:resource="http://iot.linkeddata.es/def/wot#Thing"/>
    <rdfs:label xml:lang="en">A property representing the Thing (Virtual or Physical) that a service exposes.</rdfs:label>
    <rdfs:label xml:lang="en">exposes</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#expressedInFormat">
    <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#Value"/>
    <rdfs:range rdf:resource="http://iot.linkeddata.es/def/wot#DataSchema"/>
    <rdfs:comment xml:lang="en">Link from a value to the data format in which it is expressed.</rdfs:comment>
    <rdfs:label xml:lang="en">expressed in format</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#hasComponent">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#isComponentOf">
        <rdfs:comment xml:lang="en">A link between a thing and the ecosystems it belongs to.</rdfs:comment>
        <rdfs:label xml:lang="en">is component of</rdfs:label>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:comment xml:lang="en">A link between an ecosystem and the things that compose such ecosystem.</rdfs:comment>
    <rdfs:label xml:lang="en">has component</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#hasMaxValue">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#value"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:label xml:lang="en">hasMaxValue</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#hasMinValue">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#value"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:label xml:lang="en">hasMinValue</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#hasNeighbourhood">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://iot.linkeddata.es/def/core#isInvolvedIn"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#isNeighbourhoodOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:subPropertyOf>
          <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#involves">
            <owl:inverseOf rdf:resource="http://iot.linkeddata.es/def/core#isInvolvedIn"/>
            <rdfs:isDefinedBy xml:lang="en">A link between a neighbourhood and all the agents involved in it.</rdfs:isDefinedBy>
            <rdfs:label xml:lang="en">involves</rdfs:label>
          </owl:ObjectProperty>
        </rdfs:subPropertyOf>

        <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#Neighbourhood"/>
        <rdfs:range rdf:resource="http://iot.linkeddata.es/def/core#Agent"/>
        <rdfs:isDefinedBy xml:lang="en">A link between a neighbourhood and the specific agent around which the neighbourhood is built.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">is neighbourhood of</rdfs:label>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#Agent"/>
    <rdfs:range rdf:resource="http://iot.linkeddata.es/def/core#Neighbourhood"/>
    <rdfs:isDefinedBy xml:lang="en">The link between an agent and its neighbourhood. Such neighbourhood will link to all the entities involved in such neighbourhood.</rdfs:isDefinedBy>
    <rdfs:label xml:lang="en">has neighbourhood</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#hasOwner">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#owns">
        <rdfs:isDefinedBy xml:lang="en">A linkd between an agent and the entities, like devices or services, it provides or owns.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">owns</rdfs:label>
        <rdfs:label xml:lang="en">provides</rdfs:label>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:isDefinedBy xml:lang="en">Link between an entity like a service or a device and its owners or providers.</rdfs:isDefinedBy>
    <rdfs:label xml:lang="en">has owner</rdfs:label>
    <rdfs:label xml:lang="en">has provider</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#hasPartnershipWith">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/org#Organization"/>
    <rdfs:range rdf:resource="http://www.w3.org/ns/org#Organization"/>
    <rdfs:isDefinedBy xml:lang="en">A link between two organizations who are in a partnership relation. A partnership is established between organizations, only 2 organizations.</rdfs:isDefinedBy>
    <rdfs:label xml:lang="en">hasPartnershipWith</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#hasValue">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://iot.linkeddata.es/def/wot#InteractionPattern"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://iot.linkeddata.es/def/wot#Thing"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:domain>

    <rdfs:range rdf:resource="http://iot.linkeddata.es/def/core#Value"/>
    <rdfs:comment xml:lang="en">Indicates a value provided by an interaction pattern or a Thing. In case one ore more values are indicated the order will be defined by the time stamp attributes.</rdfs:comment>
    <rdfs:label xml:lang="en">has value</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#hasVendor">
    <rdfs:isDefinedBy xml:lang="en">Link from a device to its vendor or manufacturer or producer.</rdfs:isDefinedBy>
    <rdfs:label xml:lang="en">has producer</rdfs:label>
    <rdfs:label xml:lang="en">has vendor</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#interactsUsing">
    <rdfs:isDefinedBy xml:lang="en">Linkg between an agent and the virtual thing it interatcs with.</rdfs:isDefinedBy>
    <rdfs:label xml:lang="en">interacts using</rdfs:label>
    <rdfs:seeAlso>ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)</rdfs:seeAlso>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#isComposedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdfs:domain>
      <owl:Class rdf:about="http://iot.linkeddata.es/def/core#ComposedThing">
        <owl:equivalentClass>
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://iot.linkeddata.es/def/core#isComposedBy"/>
            <owl:someValuesFrom rdf:resource="http://iot.linkeddata.es/def/wot#Thing"/>
          </owl:Restriction>
        </owl:equivalentClass>

        <rdfs:comment xml:lang="en">This class represent the things (Virtual or Physical) that are composed by other things. While Physicial things can be composed any type of thing, Virtual things can only be composed by Virtual things.</rdfs:comment>
        <rdfs:label xml:lang="en">Composed thing</rdfs:label>
      </owl:Class>
    </rdfs:domain>

    <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en">A property between a thing and its components. For example, virtual things can contain virtual things while physical things can contain physicial or virtual things.</rdfs:comment>
    <rdfs:label xml:lang="en">is composed by</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#isDescribedBy">
    <rdfs:comment xml:lang="en">Relation from a thing to its thing descriptions</rdfs:comment>
    <rdfs:label xml:lang="en">is described by</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#isInvolvedIn">
    <rdfs:isDefinedBy xml:lang="en">A link between an agent and all the neighbouthoods it is involved in.</rdfs:isDefinedBy>
    <rdfs:label xml:lang="en">is involved in</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#isLocatedAt">
    <rdfs:comment xml:lang="en">The relation between a phisical thing, for example a device, and the geographical feature in which it is located.</rdfs:comment>
    <rdfs:label xml:lang="en">is located at</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#isRelativeTo">
    <rdfs:comment xml:lang="en">Link from a relative endpoint to the (non relative) endpoint to which it is relative.</rdfs:comment>
    <rdfs:label xml:lang="en">is relative to</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#isRepresentedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#represents">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/wot#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">relation between a wot:Thing and the virtual or phyisical thing it represents</rdfs:comment>
        <rdfs:label xml:lang="en">represents</rdfs:label>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:range rdf:resource="http://iot.linkeddata.es/def/wot#Thing"/>
    <rdfs:comment xml:lang="en">relation between anything, virtual or physical, and its wot:Thing representation(s)</rdfs:comment>
    <rdfs:label xml:lang="en">is represented by</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#literalValue">
    <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#Value"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:comment xml:lang="en">Literal representation of the observed value.</rdfs:comment>
    <rdfs:label xml:lang="en">literal value</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#monitorsFeatureProperty">
    <rdfs:range rdf:resource="http://iot.linkeddata.es/def/core#FeatureProperty"/>
    <rdfs:comment xml:lang="en">Auxiliary property created for the n-ary class core:FeatureProperty. This property links devices (sensor or actuators) or interaction patterns to instances of the n-ary class which will be the ones linking to the properties and features of interest monitored.</rdfs:comment>
    <rdfs:label xml:lang="en">monitors feature property</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#petitionerItemInvolved">
    <rdfs:comment xml:lang="en">Devices or services belonging to the contract petitioner that are involved in the contract  to use the requested service.</rdfs:comment>
    <rdfs:label xml:lang="en">petitioner item involved</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#requestedService">
    <rdfs:comment xml:lang="en">Service requested in the contract</rdfs:comment>
    <rdfs:label xml:lang="en">requested service</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#serviceOwner">
    <rdfs:label xml:lang="en">Indicates the organization that owns the service involved in the contract</rdfs:label>
    <rdfs:label xml:lang="en">service owner</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://iot.linkeddata.es/def/core#servicePetitioner">
    <rdfs:comment xml:lang="en">Agent who request access to a service by a contract</rdfs:comment>
    <rdfs:label xml:lang="en">service petitioner</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.opengis.net/ont/geosparql#hasGeometry">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.opengis.net/ont/geosparql#sfContains">
    <rdfs:domain rdf:resource="http://www.opengis.net/ont/geosparql#SpatialObject"/>
    <rdfs:range rdf:resource="http://www.opengis.net/ont/geosparql#SpatialObject"/>
    <dc11:creator>OGC GeoSPARQL 1.0 Standard Working Group</dc11:creator>
    <rdfs:comment xml:lang="en">Exists if the subject SpatialObject spatially contains the object SpatialObject. DE-9IM: T*****FF*</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.opengis.net/ont/geosparql"/>
    <rdfs:label xml:lang="en">contains</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.opengis.net/ont/geosparql#sfWithin">
    <rdfs:domain rdf:resource="http://www.opengis.net/ont/geosparql#SpatialObject"/>
    <rdfs:range rdf:resource="http://www.opengis.net/ont/geosparql#SpatialObject"/>
    <dc11:creator>OGC GeoSPARQL 1.0 Standard Working Group</dc11:creator>
    <rdfs:comment xml:lang="en">Exists if the subject SpatialObject is spatially within the object SpatialObject. DE-9IM: T*F**F***</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.opengis.net/ont/geosparql"/>
    <rdfs:label xml:lang="en">within</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#value">
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:comment>Idiomatic property used for structured values.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
    <rdfs:label>value</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#location">
  </owl:ObjectProperty>

  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/observes">
    <owl:propertyChainAxiom>
      <rdf:Description>
        <rdf:first rdf:resource="http://iot.linkeddata.es/def/core#monitorsFeatureProperty"/>
        <rdf:rest>
          <rdf:Description>
            <rdf:first rdf:resource="http://iot.linkeddata.es/def/core#aboutProperty"/>
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
          </rdf:Description>
        </rdf:rest>

      </rdf:Description>
    </owl:propertyChainAxiom>

  </rdf:Description>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/forProperty">
    <owl:propertyChainAxiom>
      <rdf:Description>
        <rdf:first rdf:resource="http://iot.linkeddata.es/def/core#actsOnFeatureProperty"/>
        <rdf:rest>
          <rdf:Description>
            <rdf:first rdf:resource="http://iot.linkeddata.es/def/core#aboutProperty"/>
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
          </rdf:Description>
        </rdf:rest>

      </rdf:Description>
    </owl:propertyChainAxiom>

  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/hasDeployment">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/hasProperty">
    <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/isPropertyOf"/>
    <rdfs:comment xml:lang="en">Relation between an entity and a Property of that entity.</rdfs:comment>
    <rdfs:comment>A relation between a FeatureOfInterest and a Property of that feature.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
    <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    <rdfs:label xml:lang="en">has property</rdfs:label>
    <rdfs:label>has property</rdfs:label>
    <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton</rdfs:seeAlso>
    <skos:definition xml:lang="en">Relation between an entity and a Property of that entity.</skos:definition>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/hasSubSystem">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/implements">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/isPropertyOf">
    <rdfs:comment xml:lang="en">Relation between a Property and the entity it belongs to.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
    <rdfs:label xml:lang="en">is property of</rdfs:label>
    <skos:definition xml:lang="en">Relation between a Property and the entity it belongs to.</skos:definition>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/hasOperatingProperty">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
    <rdfs:comment>Relation from an OperatingRange to a Property.  For example, to a battery lifetime.</rdfs:comment>
    <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    <rdfs:label>has operating property</rdfs:label>
    <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction</rdfs:seeAlso>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/hasOperatingRange">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
    <rdfs:comment>Relation from a System to an OperatingRange describing the normal operating environment of the System.</rdfs:comment>
    <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    <rdfs:label>has operating range</rdfs:label>
    <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction</rdfs:seeAlso>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/hasSurvivalProperty">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
    <rdfs:comment>Relation from a SurvivalRange to a Property describing the survial range of a system.  For example, to the temperature extreme that a system can withstand before being considered damaged.</rdfs:comment>
    <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    <rdfs:label>has survival property</rdfs:label>
    <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction</rdfs:seeAlso>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/hasSurvivalRange">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
    <rdfs:comment>A Relation from a System to a SurvivalRange.</rdfs:comment>
    <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    <rdfs:label>has survival range</rdfs:label>
    <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction</rdfs:seeAlso>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/hasSystemCapability">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
    <rdfs:comment>Relation from a Sensor to a MeasurementCapability describing the measurement properties of the sensor.</rdfs:comment>
    <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    <rdfs:label>has measurement  capability</rdfs:label>
    <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability</rdfs:seeAlso>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/hasSystemProperty">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
    <rdfs:comment>Relation from a MeasurementCapability to a MeasurementProperty.  For example, to an accuracy (see notes at MeasurementCapability).</rdfs:comment>
    <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    <rdfs:label>has measurement property</rdfs:label>
    <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability</rdfs:seeAlso>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/inCondition">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/systems/qualityOfObservation">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/ssn/hasProperty"/>
    <dc11:source>skos:exactMatch 'resultQuality' [O&amp;M - ISO/DIS 19156] 
		                    http://portal.opengeospatial.org/files/?artifact_id=41579</dc11:source>
    <rdfs:comment>Relation linking an Observation to the adjudged quality of the result.  This is of course complimentary to the MeasurementCapability information recorded for the Sensor that made the Observation.</rdfs:comment>
    <rdfs:isDefinedBy>http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    <rdfs:label>quality of observation</rdfs:label>
    <rdfs:seeAlso>http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Observation#Observation</rdfs:seeAlso>
  </owl:ObjectProperty>

