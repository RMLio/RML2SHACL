    <rdfs:label xml:lang="en">Protocol independent ID</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://iot.linkeddata.es/def/core#readWrite">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#Contract"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:comment xml:lang="en">Indicates wether there are read and writes rigths over the service involved in the contrat.</rdfs:comment>
    <rdfs:comment xml:lang="en">Read and write rights</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://iot.linkeddata.es/def/core#relativeHref">
