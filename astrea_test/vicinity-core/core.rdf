    <rdfs:label xml:lang="en">Software version</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://iot.linkeddata.es/def/core#specVersion">
    <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#Device"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en">Specification version, equivalent to &quot;icv&quot; field in &quot;(IoT) DIS 30118-1 - Text for ISO/IEC DIS 30118-1, Information technology — Open Connectivity Foundation — Part 1: Core specification&quot;</rdfs:comment>
    <rdfs:label xml:lang="en">Spec Version</rdfs:label>
    <rdfs:label xml:lang="en">Specification version</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://iot.linkeddata.es/def/core#timeStamp">
