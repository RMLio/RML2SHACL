    <rdfs:comment>Device serial number</rdfs:comment>
    <rdfs:label xml:lang="en">serialNumber</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://iot.linkeddata.es/def/core#serviceDescription">
    <rdfs:subPropertyOf rdf:resource="http://iot.linkeddata.es/def/core#description"/>
    <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#Service"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en">Natural language description of a service.</rdfs:comment>
    <rdfs:label xml:lang="en">service description</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://iot.linkeddata.es/def/core#serviceId">
    <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#Service"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en">Unique dentifier of the service</rdfs:comment>
    <rdfs:label xml:lang="en">service identifier</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://iot.linkeddata.es/def/core#serviceName">
    <rdfs:subPropertyOf rdf:resource="http://iot.linkeddata.es/def/wot#name"/>
    <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#Service"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Name of a service</rdfs:comment>
    <rdfs:label xml:lang="en">service name</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://iot.linkeddata.es/def/core#serviceVersion">
    <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#Service"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en">service version</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://iot.linkeddata.es/def/core#softwareVersion">
    <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#Device"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
