    <rdfs:domain rdf:resource="http://iot.linkeddata.es/def/core#RelativeEndpoint"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en">relative href</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://iot.linkeddata.es/def/core#serialNumber">
